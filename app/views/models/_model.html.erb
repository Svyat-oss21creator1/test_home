<div id="<%= dom_id model %>">
  <p>
    <strong>Name:</strong>
    <%= model.name%>
  </p>
   <p>
    <strong>Brand:</strong>
    <%= link_to model.brand.name,  model.brand%>
  </p>     
  <p>
    <strong>Cars count:</strong>
    <%= model.cars_count %>
  </p>

  <p>
    <strong>Parent:</strong>
    <%= model.parent_id %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= model.description %>
  </p>

</div>
